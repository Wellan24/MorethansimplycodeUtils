<UserControl x:Class="GUI.Controls.ControlLineaParametro"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:local="clr-namespace:GUI.Controls"
        xmlns:gf="clr-namespace:GenericForms.Implemented;assembly=LAE.Comun" mc:Ignorable="d" x:Name="UCLineaParametro"
        BorderThickness="1" BorderBrush="DarkGray">
    <Grid Margin="10,10,40,10">
        <StackPanel>
            <TextBlock Grid.Column="1" FontFamily="Segoe UI Semibold" FontSize="15"
                    HorizontalAlignment="Center" x:Name="textHeader" TextDecorations="Underline">PUNTO DE CONTROL / FOCO</TextBlock>
            <gf:TypePanel x:Name="panelPControlNombre" />
            <gf:TypePanel x:Name="panelParametros" />
            <gf:TypeGrid x:Name="gridParametros" /> <!--MaxHeight="200"-->
            <gf:TypePanel x:Name="panelPControl" />
        </StackPanel>
    </Grid>
</UserControl>
