<UserControl x:Class="GUI.Controls.ControlFocoPlanMedicion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:local="clr-namespace:GUI.Controls"
        mc:Ignorable="d" xmlns:gf="clr-namespace:GenericForms.Implemented">
    <Expander Header="Foco" Style="{StaticResource MetroExpander}" Margin="20,20,20,0">
        <StackPanel>
            <gf:TypePanel x:Name="panelDatos" />
            <StackPanel Orientation="Horizontal" x:Name="zonasMuestreo" />
            <gf:TypePanel x:Name="panelConexion" />
            <gf:TypePanel x:Name="panelMedicion" />
            <StackPanel x:Name="lineasPuntosMuestreo" />
            <Button Click="Button_Click" Content="prueba" Margin="20" />
        </StackPanel>
    </Expander>
</UserControl>
