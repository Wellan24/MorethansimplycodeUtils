<UserControl x:Class="LAE.Biomasa.Pages.PageDensidad"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:local="clr-namespace:LAE.Biomasa.Controles"
        mc:Ignorable="d" xmlns:UControl="clr-namespace:LAE.Biomasa.Controles">
    <Grid>
        <GroupBox Header="Densidad">
            <StackPanel>
                <UControl:ControlDensidad x:Name="Prueba" />
                <Button Content="Añadir/Eliminar CCI" Click="AddDeleteCCI_Click" x:Name="botonCCI" />
                <UControl:ControlDensidad x:Name="CCI" Visibility="Collapsed" />
                <UControl:ControlDensidadCalculo x:Name="CCIAceptacion" Visibility="Collapsed" />
            </StackPanel>
        </GroupBox>
    </Grid>
</UserControl>
