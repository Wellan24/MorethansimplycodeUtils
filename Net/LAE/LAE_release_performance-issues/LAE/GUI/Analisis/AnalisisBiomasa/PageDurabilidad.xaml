<UserControl x:Class="GUI.Analisis.PageDurabilidad"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:local="clr-namespace:GUI.Analisis" mc:Ignorable="d" xmlns:UControl="clr-namespace:GUI.Analisis">
    <Grid>
        <GroupBox Header="Durabilidad/Finos">
            <StackPanel>
                <UControl:ControlDurabilidad x:Name="Prueba" />
                <Button Content="Añadir/Eliminar CCI" Click="AddDeleteCCI_Click" x:Name="botonCCI" />
                <UControl:ControlDurabilidad x:Name="CCI" Visibility="Collapsed" />
                <UControl:ControlDurabilidadCalculo x:Name="CCIAceptacion" Visibility="Collapsed" />
            </StackPanel>
        </GroupBox>
    </Grid>
</UserControl>
