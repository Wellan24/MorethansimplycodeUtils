<UserControl x:Class="GUI.Pages.TesterPage" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:local="clr-namespace:GUI.Pages"
        mc:Ignorable="d">
    <Grid x:Name="rootClientes" RenderTransformOrigin="0.675,0.527">
        <StackPanel>
            <ComboBox x:Name="comboFilter" IsEditable="True" IsTextSearchEnabled="True"
                    IsTextSearchCaseSensitive="False" MaxWidth="200" />
            <ComboBox x:Name="comboFilter2" IsEditable="True" IsTextSearchEnabled="True"
                    IsTextSearchCaseSensitive="False" MaxWidth="200" TextSearch.TextPath="Nombre"
                    DisplayMemberPath="Nombre" KeyDown="comboFilter2_KeyDown" />
            <Button x:Name="btn1" Click="Button_Click">Hola</Button>
            <Button x:Name="btn2" Click="Button_Click2">Hola2</Button>
        </StackPanel>
    </Grid>
</UserControl>
